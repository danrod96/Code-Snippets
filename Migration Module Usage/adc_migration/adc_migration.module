          $tmpfecha2 = explode("/", $tmpfecha[0]);
          $fecha_fin = $tmpfecha2[0] . "/" . $tmpfecha2[1] . "/" . $tmpfecha2[2] . $hora_final;
        }

        $tid_tipo_asesoria = get_tipo_de_asesoria($tipo_asesoria);

