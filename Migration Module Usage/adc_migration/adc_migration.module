function _get_format_date($date){
      $fecha_hora  =  explode(" ", $date);
      $fecha_array = explode("/",$fecha_hora[0]);
      $fecha       = $fecha_array[2].'-'.$fecha_array[1].'-'.$fecha_array[0].' '.$fecha_hora[1];
      return $fecha;

}

