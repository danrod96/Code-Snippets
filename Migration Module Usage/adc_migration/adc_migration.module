  while (($line = fgetcsv($file)) !== FALSE) {

    $id = $line[0];

    if($line[0]){

      $id = str_replace(".", "", $id);
      $id = str_replace(",", "", $id);

      $id = (int) $id;

      if ($id == "id_contactenos"){
        continue;
      }

